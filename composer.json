{
  "name": "gschurgast/eudr-api-client",
  "description": "Minimal EUDR SOAP client utilities and functional test",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-dom": "*",
    "ext-soap": "*",
    "robrichards/wse-php": "^2.0",
    "symfony/yaml": "^7.0",
    "webmozart/assert": "^1.12"
  },
  "autoload": {
    "psr-4": {
      "src\\": "src/"
    }
  },
  "scripts": {
    "demo:migration": "php tests/migration_demo.php"
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.89",
    "phpstan/phpstan": "^2.1"
  }
}
